extends layout

block content
  .page-wrapper
    .container
      .row.page-titles
        .col-md-6.col-8.align-self-center
          h3.text-themecolor.m-b-0.m-t-0 Dashboard
          ol.breadcrumb
            li.breadcrumb-item
              a(href='javascript:void(0)') Home
            li.breadcrumb-item.active Dashboard
        .col-md-6.col-4.align-self-center
          a.btn.pull-right.hidden-sm-down.btn-success(href='https://themewagon.com/themes/free-bootstrap-4-admin-dashboard-template/') Download Here
      .row
        .col-sm-6
          .card
            .card-block
              h4.card-title Daily Sales
              .text-right
                h2.font-light.m-b-0
                  i.ti-arrow-up.text-success
                span.text-muted Todays Income
              span.text-success 80%
              .progress
                .progress-bar.bg-success(role='progressbar', style='width: 80%; height: 6px;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
        .col-sm-6
          .card
            .card-block
              h4.card-title Weekly Sales
              .text-right
                h2.font-light.m-b-0
                  i.ti-arrow-up.text-info
                span.text-muted Todays Income
              span.text-info 30%
              .progress
                .progress-bar.bg-info(role='progressbar', style='width: 30%; height: 6px;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
      .row
        .col-sm-12
          .card
            .card-block
              h4.card-title Revenue Statistics
              .flot-chart
                #flot-line-chart.flot-chart-content
      .row
        .col-sm-12
          .card
            .card-block
              select.custom-select.pull-right
                option(selected='') January
                option(value='1') February
                option(value='2') March
                option(value='3') April
              h4.card-title Projects of the Month
              .table-responsive.m-t-40
                table.table.stylish-table
                  thead
                    tr
                      th(colspan='2') Assigned
                      th Name
                      th Budget
                  tbody
                    tr
                      td(style='width:50px;')
                        span.round S
                      td
                        h6 Sunil Joshi
                        small.text-muted Web Designer
                      td Elite Admin
                      td $3.9K
                    tr.active
                      td
                        span.round
                          img(src='assets/images/users/2.jpg', alt='user', width='50')
                      td
                        h6 Andrew
                        small.text-muted Project Manager
                      td Real Homes
                      td $23.9K
                    tr
                      td
                        span.round.round-success B
                      td
                        h6 Bhavesh patel
                        small.text-muted Developer
                      td MedicalPro Theme
                      td $12.9K
                    tr
                      td
                        span.round.round-primary N
                      td
                        h6 Nirav Joshi
                        small.text-muted Frontend Eng
                      td Elite Admin
                      td $10.9K
                    tr
                      td
                        span.round.round-warning M
                      td
                        h6 Micheal Doe
                        small.text-muted Content Writer
                      td Helping Hands
                      td $12.9K
                    tr
                      td
                        span.round.round-danger N
                      td
                        h6 Johnathan
                        small.text-muted Graphic
                      td Digital Agency
                      td $2.6K
      .row
        .col-lg-4
          .card
            img.card-img-top.img-responsive(src='assets/images/big/img1.jpg', alt='Card')
            .card-block
              ul.list-inline.font-14
                li.p-l-0 20 May 2016
                li
                  a.link(href='javascript:void(0)') 3 Comment
              h3.font-normal Featured Hydroflora Pots Garden &amp; Outdoors
        .col-lg-4
          .card
            img.card-img-top.img-responsive(src='assets/images/big/img2.jpg', alt='Card')
            .card-block
              ul.list-inline.font-14
                li.p-l-0 20 May 2016
                li
                  a.link(href='javascript:void(0)') 3 Comment
              h3.font-normal Featured Hydroflora Pots Garden &amp; Outdoors
        .col-lg-4
          .card
            img.card-img-top.img-responsive(src='assets/images/big/img4.jpg', alt='Card')
            .card-block
              ul.list-inline.font-14
                li.p-l-0 20 May 2016
                li
                  a.link(href='javascript:void(0)') 3 Comment
              h3.font-normal Featured Hydroflora Pots Garden &amp; Outdoors
    footer.footer.text-center